<section class="sweetsParallax">
    <img width="200px" height="200px" src="data:image/svg+xml;utf8;base64," />
</section>


<main>
  <div class="container"> 
    <section class="sweets-ops">
        <div class="options " id="a">
          <div >
        <h2>Add Sweets</h2>
        <form class="create-update-form" action="/sweets" method="POST" name="form" class="form">
            <div class="form-group">
                <label>Sweets Name</label>
                <input type="text" name="name" class="drawn-border" id="formVal">
                <button class="btn btn-warning btn-tap" type="submit">Add Sweets</button>
            </div>
        </form>
        </div>
        <br>
      </div><br>
        <div class="options available " id="b">
            <div >
                <h2>Available Sweets</h2>
                <ul>
                  {{#each sweets}}
                   {{#unless this.devoured}}
                      <li class="sweets-status">
                        {{this.sweets_name}} 
                        <form action="/sweets{{this.id}}?_method=PUT" method="POST">
                          <input type="hidden" name="id" value="{{this.id}}" class="drawn-border" id="formVal">
                          <button id="grid-snap" type="submit" class="btn btn-warning btn-tap">Devour!</button>
                        </form>
                      </li>
                    {{/unless}}
                  {{/each}}
                </ul>
            </div>
        <br>
        </div><br>
        <div class="options devoured " id="c">
          <div>
            <h2>Devoured Sweets</h2>
            
            <ul class="sweets-status">
              {{#each sweets}}
                {{#if this.devoured}}
                  <li>
                    {{this.sweets_name}}
                  </li>
                {{/if}}
              {{/each}}
            </ul>
        </div>
        <br>
      </div><br>
    </section>
  </div>{{#each sweets}}
  <footer align="center"></footer>
  {{/each}}
</main>       